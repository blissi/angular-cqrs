{
  "name": "angular-cqrs",
  "version": "0.0.1",
  "description": "Provides CQRS functionality for Angular 2/4.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c && copy package.dist.json dist\\package.json",
    "minify": "uglifyjs dist/bundles/angular-cqrs.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/angular-cqrs.umd.min.js",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blissi/angular-cqrs.git"
  },
  "keywords": [
    "Angular",
    "CQRS"
  ],
  "author": "Steven Wei√ü",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blissi/angular-cqrs/issues"
  },
  "homepage": "https://github.com/blissi/angular-cqrs#readme",
  "dependencies": {
    "@angular/common": "^5.0.5",
    "@angular/core": "^5.0.5",
    "dottie": "^2.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler": "^5.1.0",
    "@angular/compiler-cli": "^5.1.0",
    "@types/chai": "^4.0.6",
    "@types/dottie": "^2.0.2",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.0.0",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "copy": "^0.3.1",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "sinon": "^4.1.2",
    "typescript": "^2.6.2",
    "uglify-js": "^3.2.1"
  }
}
